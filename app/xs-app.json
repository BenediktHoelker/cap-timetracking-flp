{
    "welcomeFile": "/cp.portal",
    "authenticationMethod": "route",
    "logout": {
        "logoutEndpoint": "/do/logout"
    },
    "routes": [
        {
            "source": "^/cap-app/(.*)$",
            "target": "$1",
            "localDir": "cap-app/"
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "authenticationType": "xsuaa",
            "destination": "srv_api"
        }
    ]
}