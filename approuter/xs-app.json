{
  "welcomeFile": "../webapp/flpSandbox.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/ui-customers/(.*)$",
      "target": "$1",
      "localDir": "webapp/"
    },
    {
      "source": "^/(.*)",
      "destination": "srv_api"
    }
  ]
}